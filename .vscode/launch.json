{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": [],
      "env": {},
      "showLog": true,
      "cwd": "${workspaceFolder}",
      "buildFlags": "",
